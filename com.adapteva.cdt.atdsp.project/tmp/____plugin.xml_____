   <extension
         id="EGCCManagedMakePerProjectProfileC"
         name="Epiphany Setting"
         point="org.eclipse.cdt.make.core.ScannerConfigurationDiscoveryProfile">
      <scannerInfoCollector
            class="com.adapteva.cdt.epiphany.project.scanerconfig.EDefaultGCCScannerInfoCollector"
            scope="project"/>
      <buildOutputProvider>
         <open/>
         <scannerInfoConsoleParser
               class="com.adapteva.cdt.epiphany.project.scanerconfig.EManagedGCCScannerInfoConsoleParser"
               compilerCommands="e-gcc"/>
      </buildOutputProvider>
      <scannerInfoProvider providerId="specsFile">
         <run
