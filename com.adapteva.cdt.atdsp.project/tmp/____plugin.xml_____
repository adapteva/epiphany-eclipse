<plugin>
   
   
      <extension
         point="org.eclipse.cdt.managedbuilder.core.buildDefinitions">
      <managedBuildRevision
            fileVersion="4.0.0">
      </managedBuildRevision>
      
      <tool
            command="e-gcc"
            id="cdt.managedbuild.tool.gnu.c.compiler.atdsp"
            name="Epiphany compiler"
            superClass="cdt.managedbuild.tool.gnu.c.compiler">
         <envVarBuildPath
               buildPathResolver="com.adapteva.cdt.epiphany.project.EpiphanyPathResolver"
               pathType="buildpathInclude"
               variableList="EPIPHANY_HOME">
         </envVarBuildPath>
         <optionCategory
               id="atdsp.com.cdt.atdsp.project.optionCategory3"
               name="Target specific">
         </optionCategory>
         <option
               category="atdsp.com.cdt.atdsp.project.optionCategory3"
               command="-mfused-madd"
               defaultValue="-mfused-madd"
               id="atdsp.com.cdt.atdsp.project.option2"
               isAbstract="false"
               name="Generate fused multiply/add instructions"
               tip="turn on optimization to enbale this option"
               value="true"
               valueType="boolean">
         </option>
         <option
               category="atdsp.com.cdt.atdsp.project.optionCategory3"
               defaultValue="-mlong-calls"
               id="com.adapteva.cdt.atdsp.project.option1"
               isAbstract="false"
               name="Generate call insns as direct/indirect calls"
               valueType="enumerated">
            <enumeratedOptionValue
                  command="-mlong-calls"
                  id="com.adapteva.cdt.atdsp.project.enumeratedOptionValue1"
                  isDefault="true"
                  name="indirect call">
            </enumeratedOptionValue>
            <enumeratedOptionValue
                  command="-mshort-calls"
                  id="com.adapteva.cdt.atdsp.project.enumeratedOptionValue2"
                  isDefault="false"
                  name="direct call">
            </enumeratedOptionValue>
         </option>
         <option
               category="atdsp.com.cdt.atdsp.project.optionCategory3"
               command="-mfast-div"
               defaultValue="-mfast-div"
               id="com.adapteva.cdt.atdsp.project.option2"
               isAbstract="false"
               name="Use the fast floating point divide implementation"
               resourceFilter="all"
               tip="-mfast-div"
               value="false"
               valueType="boolean">
         </option>
         <option
               category="atdsp.com.cdt.atdsp.project.optionCategory3"
               command="-ffunction-sections -fdata-sections"
               defaultValue="-ffunction-sections -fdata-sections"
               id="Force remove unused "
               isAbstract="false"
               name="Force remove unused function/code"
               resourceFilter="all"
               tip="Place each function or data item into its own section in the output file. "
               value="false"
               valueType="boolean">
         </option>
      </tool>
      <tool
            command="e-gcc"
            id="cdt.managedbuild.tool.gnu.c.linker.atdsp"
            name="Epiphany linker"
            natureFilter="cnature"
            superClass="cdt.managedbuild.tool.gnu.c.linker">
         <optionCategory
               id="com.adapteva.cdt.epiphany.project.optionCategory1"
               name="Target specific">
         </optionCategory>
         <option
               category="com.adapteva.cdt.epiphany.project.optionCategory1"
               command="-Wl,--gc-sections"
               defaultValue="-Wl,--gc-sections"
               id="gcsection"
               isAbstract="false"
               name="Enable garbage collection of unused input sections"
               resourceFilter="all"
               tip="Enable garbage collection of unused input sections by by examining symbols and relocations."
               value="false"
               valueType="boolean">
         </option>
         <option
               category="com.adapteva.cdt.epiphany.project.optionCategory1"
               id="com.adapteva.cdt.epiphany.project.option1"
               isAbstract="false"
               name="Select the emulation platform"
               resourceFilter="all"
               valueType="enumerated">
            <enumeratedOptionValue
                  command="-Xlinker -melf32epiphany_4x4"
                  id="com.adapteva.cdt.epiphany.project.enumeratedOptionValue1"
                  isDefault="true"
                  name="elf32epiphany_4x4">
            </enumeratedOptionValue>
            <enumeratedOptionValue
                  command="-Xlinker -melf32epiphany"
                  id="com.adapteva.cdt.epiphany.project.enumeratedOptionValue2"
                  isDefault="false"
                  name="elf32epiphany">
            </enumeratedOptionValue>
         </option>
      </tool>
      <tool
            command="e-as"
            errorParsers="org.eclipse.cdt.core.GASErrorParser"
            id="cdt.managedbuild.tool.gnu.assembler.atdsp"
            name="Epiphany assembler"
            outputFlag="-o"
            superClass="cdt.managedbuild.tool.gnu.assembler">
      </tool>
      <tool
            command="e-ar"
            id="cdt.managedbuild.tool.gnu.archiver.atdsp"
            name="Epiphany archiver"
            superClass="cdt.managedbuild.tool.gnu.archiver">
      </tool>
      <tool
            command="e-objcopy"
            commandLinePattern="${COMMAND} ${FLAGS} ${OUTPUT_FLAG} ${INPUTS} ${OUTPUT}"
            id="cdt.managedbuild.tool.gnu.c.objcopy.atdsp"
            isAbstract="false"
            name="Epiphany objcopy"
            outputFlag="--srec-forceS3 --verbose --output-target srec">
         <optionCategory
               id="atdsp.com.cdt.atdsp.project.optionCategoryGeneral"
               name="General">
         </optionCategory>
         <option
               category="atdsp.com.cdt.atdsp.project.optionCategoryGeneral"
               command="-verbose"
               id="atdsp.com.cdt.atdsp.project.verbose"
               isAbstract="false"
               name="verbose"
               resourceFilter="all"
               valueType="boolean">
         </option>
      </tool>
      
      
      
      
      
      
      <toolChain
            archList="all"
            id="cdt.managedbuild.toolchain.gnu.atdsp.base"
            name="Epiphany"
            osList="all"
