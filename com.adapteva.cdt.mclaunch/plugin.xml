<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="com.adapteva.cdt.mclaunch.AbstractLaunchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup"
            type="com.adapteva.cdt.mclaunch.launchConfigurationType">
         <launchMode
               description="RUN MODE"
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.adapteva.cdt.mclaunch.AbstractLaunchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTabGroupServer"
            type="com.adapteva.cdt.mclaunch.launchConfigurationTypeEserver">
         <launchMode
               description="RUN MODE"
               mode="run">
         </launchMode>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.adapteva.cdt.mclaunch.AbstractLaunchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            type="com.adapteva.cdt.mclaunch.launchConfigurationType2">
         <launchMode
               description="DEBUG MODE"
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
      </launchConfigurationTabGroup>
      <launchConfigurationTabGroup
            class="com.adapteva.cdt.mclaunch.AbstractLaunchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            type="com.adapteva.cdt.mclaunch.launchConfigurationType1">
         <launchMode
               description="DEBUG MODE"
               mode="debug"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabs">
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTabEserverOptions"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroupServer"
            id="com.adapteva.cdt.mclaunch.tabserver"
            name="E-server">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.eserver">
         </associatedDelegate>
      </tab>
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTab1"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup"
            id="com.adapteva.cdt.mclaunch.tab1"
            name="Main">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.loader">
         </associatedDelegate>
      </tab>
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTabLoaderOptions"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup"
            id="com.adapteva.cdt.mclaunch.tab2"
            name="E-loader options">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.loader">
         </associatedDelegate>
         <placement
               after="com.adapteva.cdt.mclaunch.tab1">
         </placement>
      </tab>
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTabTarget"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.gdb.target"
            name="Targets">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.mcdebug">
         </associatedDelegate>
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.scdebug">
         </associatedDelegate>
      </tab>
      <tab
            class="org.eclipse.debug.ui.sourcelookup.SourceLookupTab"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.gdb.source"
            name="Source">
         <placement
               after="com.adapteva.cdt.mclaunch.gdb.target">
         </placement>
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.mcdebug">
         </associatedDelegate>
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.scdebug">
         </associatedDelegate>
      </tab>
      <tab
            class="org.eclipse.debug.ui.CommonTab"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.gdb.common"
            name="Common">
         <placement
               after="com.adapteva.cdt.mclaunch.gdb.source">
         </placement>
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.mcdebug">
         </associatedDelegate>
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.scdebug">
         </associatedDelegate>
      </tab>
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTab1"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.gdb.main"
            name="Main">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.scdebug">
         </associatedDelegate>
         <placement
               after="com.adapteva.cdt.mclaunch.gdb.common">
         </placement>
      </tab>
      <tab
            class="com.adapteva.cdt.mclaunch.LaunchConfigurationTabProjectsSelection"
            group="com.adapteva.cdt.mclaunch.launchConfigurationTabGroup1"
            id="com.adapteva.cdt.mclaunch.gdb.projects"
            name="Projects">
         <associatedDelegate
               delegate="com.adapteva.cdt.mclaunch.launch.mcdebug">
         </associatedDelegate>
         <placement
               after="com.adapteva.cdt.mclaunch.gdb.common">
         </placement>
      </tab>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            id="com.adapteva.cdt.mclaunch.launchConfigurationTypeEserver"
            modes="run"
            name="Epiphany Multicore Server (e-server)"
            public="true">
      </launchConfigurationType>
      <launchConfigurationType
            id="com.adapteva.cdt.mclaunch.launchConfigurationType"
            modes="run"
            name="Epiphany Multicore Applications (loader)"
            public="true">
      </launchConfigurationType>
      <launchConfigurationType
            id="com.adapteva.cdt.mclaunch.launchConfigurationType2"
            modes="debug"
            name="Epiphany Multicore Applications (gdb)"
            public="true">
      </launchConfigurationType>
      <launchConfigurationType
            id="com.adapteva.cdt.mclaunch.launchConfigurationType1"
            modes="debug"
            name="Epiphany Single Core Application (gdb)"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="com.adapteva.cdt.mclaunch.launchConfigurationType"
            icon="icons/favicon.gif"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTypeImage1">
      </launchConfigurationTypeImage>
      <launchConfigurationTypeImage
            configTypeID="com.adapteva.cdt.mclaunch.launchConfigurationType2"
            icon="icons/favicon.gif"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTypeImage2">
      </launchConfigurationTypeImage>
      <launchConfigurationTypeImage
            configTypeID="com.adapteva.cdt.mclaunch.launchConfigurationType1"
            icon="icons/favicon.gif"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTypeImage3">
      </launchConfigurationTypeImage>
      <launchConfigurationTypeImage
            configTypeID="com.adapteva.cdt.mclaunch.launchConfigurationTypeEserver"
            icon="icons/favicon.gif"
            id="com.adapteva.cdt.mclaunch.launchConfigurationTypeImage4">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchDelegates">
      <launchDelegate
            delegate="com.adapteva.cdt.mclaunch.LaunchConfigurationDelegateLoader"
            id="com.adapteva.cdt.mclaunch.launch.loader"
            modes="run"
            name="E-Loader: Epiphany Multicore Application "
            type="com.adapteva.cdt.mclaunch.launchConfigurationType">
      </launchDelegate>
      <launchDelegate
            delegate="com.adapteva.cdt.mclaunch.LaunchConfigurationDelegateEServer"
            id="com.adapteva.cdt.mclaunch.launch.eserver"
            modes="run"
            name="E-Server: Epiphany Server"
            type="com.adapteva.cdt.mclaunch.launchConfigurationTypeEserver">
      </launchDelegate>
      <launchDelegate
            delegate="com.adapteva.cdt.mclaunch.EGDBMultiLaunchConfigurationDelegate"
            id="com.adapteva.cdt.mclaunch.launch.mcdebug"
            modes="debug"
            name="GDB: Epiphany Multicore Applications "
            sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
            sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer"
            type="com.adapteva.cdt.mclaunch.launchConfigurationType2">
      </launchDelegate>
      <launchDelegate
            delegate="com.adapteva.cdt.mclaunch.EGDBSingleCoreLaunchConfigurationDelegate"
            id="com.adapteva.cdt.mclaunch.launch.scdebug"
            modes="debug"
            name="GDB: Epiphany Single Core Application"
            sourceLocatorId="org.eclipse.cdt.debug.core.sourceLocator"
            sourcePathComputerId="org.eclipse.cdt.debug.core.sourcePathComputer"
            type="com.adapteva.cdt.mclaunch.launchConfigurationType1">
      </launchDelegate>
   </extension>
   <extension
         point="org.eclipse.debug.ui.debugModelPresentations">
      <debugModelPresentation
            class="com.adapteva.cdt.mclaunch.proxy.debugger.MCDebugModelPresentation"
            id="com.adapteva.cdt.mclaunch.debugModelPresentation">
      </debugModelPresentation>
   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            description="Run e-server"
            id="com.adapteva.cdt.mclaunch.actionSet"
            label="Run e-server"
            visible="true">
         <menu
               icon="icons/images_small.gif"
               id="EServerMenu"
               label="Run e-serverMenu">
            <separator
                  name="EserverGroup">
            </separator>
         </menu>
         <action
               class="com.adapteva.cdt.mclaunch.actions.RunEserver"
               icon="icons/images_small.gif"
               id="com.adapteva.cdt.mclaunch.actions.RuneServer"
               label="Run e-servertion"
               toolbarPath="EserverGroup"
               tooltip="Run e-server">
         </action>
      </actionSet>
   </extension>

</plugin>
